<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <font
        android:font="@font/texgyreadventor_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/texgyreadventor_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:targetApi="26" />

    <font
        android:font="@font/texgyreadventor_italic"
        android:fontStyle="italic"
        android:fontWeight="400"

        app:font="@font/texgyreadventor_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        tools:targetApi="26" />

    <font
        android:font="@font/texgyreadventor_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/texgyreadventor_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:targetApi="26" />

    <font
        android:font="@font/texgyreadventor_bolditalic"
        android:fontStyle="italic"
        android:fontWeight="700"

        app:font="@font/texgyreadventor_bolditalic"
        app:fontStyle="italic"
        app:fontWeight="700"
        tools:targetApi="26" />

</font-family>